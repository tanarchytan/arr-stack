# Homepage Environment Variables
# Copy this file to /opt/data/media/appdata/homepage/.env and fill in your values

# Your server IP address (e.g., 192.168.1.100 or localhost)
HOMEPAGE_VAR_SERVER_IP=localhost

# API Keys - Get these from Settings -> General in each application
HOMEPAGE_VAR_SONARR_API_KEY=your_sonarr_api_key_here
HOMEPAGE_VAR_RADARR_API_KEY=your_radarr_api_key_here
HOMEPAGE_VAR_LIDARR_API_KEY=your_lidarr_api_key_here
HOMEPAGE_VAR_BAZARR_API_KEY=your_bazarr_api_key_here
HOMEPAGE_VAR_PROWLARR_API_KEY=your_prowlarr_api_key_here
HOMEPAGE_VAR_OVERSEERR_API_KEY=your_overseerr_api_key_here
HOMEPAGE_VAR_SABNZBD_API_KEY=your_sabnzbd_api_key_here

# qBittorrent credentials
HOMEPAGE_VAR_QBITTORRENT_USER=admin
HOMEPAGE_VAR_QBITTORRENT_PASS=your_qbittorrent_password_here
